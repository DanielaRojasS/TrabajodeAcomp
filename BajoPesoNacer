      "cell_type": "code",
      "source": [
        "#poner rangos de edad\n",
        "\n",
        "def rangos_edad(edad):\n",
        "  if edad < 15:\n",
        "    return \"0-14\"\n",
        "  elif edad < 25:\n",
        "    return \"15-24\"\n",
        "  elif edad < 35:\n",
        "    return \"25-34\"\n",
        "  elif edad < 45:\n",
        "    return \"35-44\"\n",
        "  else:\n",
        "    return \"45 o mas\"\n",
        "\n",
        "df['rango_edad_'] = df['edad_'].apply(rangos_edad)\n",
        "print(df.head())"
      ],
